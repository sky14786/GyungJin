<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "Mapper" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.gj.common.mapper.TradeCategoryMapper">
	<select id="findAll" resultType="tradecategory">
		SELECT * FROM TRADE_CATEGORY
	</select>
	
	<insert id="insert" parameterType="map">
		INSERT INTO TRADE_CATEGORY VALUES(DEFAULT,#{tradeName})
	</insert>
</mapper>
